import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        int n = scan.nextInt();
        double[] scores;
        double sum;
        double average;
        double[] arr;
        
        for (int i = 0; i < n; i++) {
            int stud = scan.nextInt();
            int index = 0;
            sum = 0;
            average = 0;
            scroes = new double[stud];
            arr = new double[stud];
            
            
                for (int k =0; i < stud; i++ ) {
                    if (scan.hasNextInt()) {
                        arr[k] = scan.nextInt();
                        sum += arr[k];
                        scores[k] = arr[k];
                    }
                }
                average = sum / stud;
                if (int k = 0; k < stud; k++) {
                    if (scores[k] > average) {
                        index++;
                    }
                }
            
            
            double rate = (double)index / stud * 100;
            System.out.printf("%.3f", rate);
            System.out.println("%");
            
        }
    }
}