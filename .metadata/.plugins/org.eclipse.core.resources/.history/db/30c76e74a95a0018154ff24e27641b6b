import java.util.*;

public class Main {
	static int[][] graph = new int[100][100];
	static boolean[][] visited = new boolean[100][100];
	static int n = 0;
	static int m = 0;
//	static int len[] = {0,0,-1,-1};
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner scan = new Scanner(System.in);
		
		n = scan.nextInt(); //4
		m = scan.nextInt(); //5
		
		for (int i = 0; i < n; i++) {
			String ps = scan.next();
			for (int j = 0; j < 2; j++) {
				graph[i][j] = Integer.parseInt(ps.charAt(j) + "");
			}
		}
		for (int i = 0; i < n; i++) {
//			String ps = scan.next();
			for (int j = 0; j < 2; j++) {
				System.out.println(graph[i][j]);
			}
		}
	}
}
